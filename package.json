{
  "name": "vue-pure-template",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "mock": "vite --mode mock",
    "taze": "taze",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "format": "npm run prettier && npm run lint",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "mockjs": "^1.1.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.0.4",
    "@iconify-json/uil": "^1.1.2",
    "@types/node": "^18.7.14",
    "@vitejs/plugin-vue": "^3.0.3",
    "eslint": "^8.23.0",
    "husky": "^8.0.0",
    "sass": "^1.54.8",
    "taze": "^0.8.1",
    "typescript": "^4.8.2",
    "unocss": "^0.45.13",
    "vite": "^3.0.9",
    "vite-plugin-mock": "^2.9.6",
    "vitest": "^0.22.1",
    "vue-tsc": "^0.39.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    }
  }
}
